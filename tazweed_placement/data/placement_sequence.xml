<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_placement_client" model="ir.sequence">
            <field name="name">Placement Client</field>
            <field name="code">tazweed.client</field>
            <field name="prefix">CLI-</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_client_contract" model="ir.sequence">
            <field name="name">Client Contract</field>
            <field name="code">tazweed.client.contract</field>
            <field name="prefix">CON-</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_job_order" model="ir.sequence">
            <field name="name">Job Order</field>
            <field name="code">tazweed.job.order</field>
            <field name="prefix">JO-%(year)s-</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_candidate" model="ir.sequence">
            <field name="name">Candidate</field>
            <field name="code">tazweed.candidate</field>
            <field name="prefix">CAN-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_placement" model="ir.sequence">
            <field name="name">Placement</field>
            <field name="code">tazweed.placement</field>
            <field name="prefix">PLC-%(year)s-</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_deployment" model="ir.sequence">
            <field name="name">Deployment</field>
            <field name="code">tazweed.deployment</field>
            <field name="prefix">DEP-</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_client_invoice" model="ir.sequence">
            <field name="name">Client Invoice</field>
            <field name="code">tazweed.client.invoice</field>
            <field name="prefix">CINV-%(year)s-</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
