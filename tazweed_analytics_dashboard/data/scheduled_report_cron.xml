<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!-- Cron Job for Scheduled Reports -->
    <record id="ir_cron_scheduled_reports" model="ir.cron">
        <field name="name">Analytics: Execute Scheduled Reports</field>
        <field name="model_id" ref="model_analytics_scheduled_report"/>
        <field name="state">code</field>
        <field name="code">model._cron_execute_scheduled_reports()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
    
</odoo>
